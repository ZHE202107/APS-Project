<?xml version="1.0" encoding="utf-8"?>
<!-- 垂直虛線 -->
<!-- 實現：套用水平虛線樣式、並旋轉90度 -->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="-600dp"
        android:right="-600dp">
        <rotate android:drawable="@drawable/dash_horizon_line"
                android:visible="true"
                android:fromDegrees="90"/>
    </item>
</layer-list>